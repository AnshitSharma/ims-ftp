			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Response format is valid\", function () {\r",
							"pm.test(\"Response format is valid\", function () {\r",
							"    const response = pm.response.json();\r",
							"    pm.expect(response).to.have.all.keys('success', 'authenticated', 'message', 'timestamp', 'code', 'data');\r",
							"    pm.expect(response.data).to.have.all.keys('user', 'tokens', 'permissions');\r",
							"    pm.expect(response.data.user).to.have.all.keys('id', 'username', 'email', 'firstname', 'lastname');\r",
							"    pm.expect(response.data.tokens).to.have.all.keys('access_token', 'refresh_token', 'expires_in');\r",
							"    pm.expect(response.data.permissions).to.be.an('array');\r",
							"});    const response = pm.response.json();\r",
							"    \r",
							"    pm.expect(response).to.have.all.keys('success', 'authenticated', 'message', 'timestamp', 'code', 'data');\r",
							"    pm.expect(response.success).to.be.a('boolean');\r",
							"    pm.expect(response.authenticated).to.be.a('boolean');\r",
							"    pm.expect(response.message).to.be.a('string');\r",
							"    pm.expect(response.timestamp).to.be.a('string');\r",
							"    pm.expect(response.code).to.be.a('number');\r",
							"    pm.expect(response.data).to.be.an('object');\r",
							"});\r",
							"pm.test(\"Access token matches expected value\", function () {\r",
							"    const response = pm.response.json();\r",
